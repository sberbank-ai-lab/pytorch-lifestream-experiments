{
  "params": {
    "experiment": {
      "name": "default"
    },
    "device": "cuda",
    "rnn": {
      "type": "gru",
      "hidden_size": 256,
      "bidir": false,
      "trainable_starter": "static"
    },
    "trx_encoder": {
      "norm_embeddings": false,
      "embeddings_noise": 0.003,
      "embeddings": {
        "trans_date": {"in": 800, "out": 16},
        "small_group": {"in": 250, "out": 16}
      },
      "numeric_values": {
        "amount_rur": "identity"
      }
    },
    "lr_scheduler": {
      "step_size": 30,
      "step_gamma": 0.9025
    },
    "train": {
      "num_workers": 16,
      "batch_size": 512,
      "split_strategy": {
        "split_strategy": "SplitByCount",
        "split_count": 5,
        "cnt_min": 25,
        "cnt_max": 150
      },
      "sampling_strategy": "HardNegativePair",
      "trx_dropout": 0.01,
      "max_seq_len": 1200,
      "neg_count": 5,
      "loss": "ContrastiveLoss",
      "margin": 0.5,
      "lr": 0.002,
      "weight_decay": 0.0,
      "n_epoch": 150
    },
    "valid": {
      "batch_size": 1024,
      "num_workers": 16,
      "split_strategy": {
        "split_strategy": "SplitByCount",
        "split_count": 5,
        "cnt_min": 25,
        "cnt_max": 100
      },
      "max_seq_len": 1200
    }
  },

  "save_model": true,
  "model_path": {
    "model": "models/age_pred_ml_model.p"
  }
}
