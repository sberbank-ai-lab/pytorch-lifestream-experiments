{
    embedding_dim: 256
    print_stat: false

    data_prepare: {
        min_trx_per_edge: 5
    }
    epoch_n: 10
    batch_size: 16
    valid: {
        batch_size: 1024
        neg_sampling_rate: 16
    }
    num_workers: 12
    max_node_count: 800
    tree_batching_level: 2
    distance: l2
    loss: {
        name: ContrastiveLoss
        pos_margin: 0.1
        neg_margin: 1.0
        total_node_count: 100000
    }
    optimizer: {
        name: SGD
        lr: 0.0001
    }
}