{
  "params": {
    "device": "cuda",

    "model_type": "transf",
    "ensemble_size": 1,
    "trx_encoder": {
      "embeddings": {
        "trans_date": {"in": 800},
        "small_group": {"in": 250}
      },
      "numeric_values": {
        "amount_rur": "identity"
      }
    },
    "transf": {
      "input_size": 256
    },
    "train": {
      "random_neg": false,
      "trx_dropout": 0.005,
      "max_seq_len": 1200,
      "SubsamplingDataset": {
        "min_seq_len_s": 300,
        "min_seq_len_e": 300,
        "max_seq_len_s": 700,
        "max_seq_len_e": 700,
        "crop_proba_init": 1.0,
        "crop_proba_gamma": 1.0,
        "total_n_epoch": 15
      },
      "batch_size": 16,
      "num_workers": 16,
      "loss": "NLLLoss",
      "lr": 0.00005,
      "weight_decay": 0.0,
      "n_epoch": 6
    },
    "lr_scheduler": {
      "step_size": 1,
      "step_gamma": 0.6
    },
    "valid": {
      "max_seq_len": 1200,
      "batch_size": 16,
      "num_workers": 8
    },
    "norm_scores": false,
    "score_metric": "accuracy",

    "pretrained_model_path": "models/transf_model.p",
    "use_batch_norm": false
  },

  "save_model": false,

  "stats": {
    "path": "runs/results.json"
  },

  "output": {
    "valid": {
      "path": "data/transf_finetuning_scores/valid",
      "format": "pickle"
    },
    "test": {
      "path": "data/transf_finetuning_scores/test",
      "format": "pickle"
    },
    "columns": ["client_id"]
  }
}
