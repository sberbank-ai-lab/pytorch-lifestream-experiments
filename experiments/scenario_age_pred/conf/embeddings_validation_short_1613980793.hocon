environment {
  work_dir = "embeddings_validation.work"
}
report_file = "../results/scenario_age_pred__hidden_size.txt"
target {
  file_name = "../data/train_target.csv"
  cols_id = [
    "client_id"
  ]
  cols_id_type = [
    "int"
  ]
  col_target = "bins"
}
split {
  fit_ids = true
  train_id {
    file_name = "../data/train_target.csv"
  }
  test_id {
    file_name = "../data/test_ids_file.csv"
  }
  cv_split_count = 5
  is_stratify = true
  random_state = 42
}
features {
  mles_embeddings {
    enabled = true
    read_params {
      file_name = "../data/mles_embeddings.pickle"
    }
    target_options {}
  }
}
external_scores {}
models {
  lgbm {
    enabled = true
    cls_name = "lightgbm.LGBMClassifier"
    params {
      n_estimators = 1000
      boosting_type = "gbdt"
      objective = "multiclass"
      num_class = 4
      metric = "multi_error"
      learning_rate = 0.02
      subsample = 0.75
      subsample_freq = 1
      feature_fraction = 0.75
      colsample_bytree = null
      max_depth = 12
      lambda_l1 = 1
      reg_alpha = null
      lambda_l2 = 1
      reg_lambda = null
      min_data_in_leaf = 50
      min_child_samples = null
      num_leaves = 50
      random_state = 42
      n_jobs = 4
    }
    cpu_count = 4
    preprocessing = [
      [
        "sklearn.preprocessing.MaxAbsScaler"
        {}
      ]
    ]
  }
}
metrics {
  accuracy {
    enabled = true
    score_func = "sklearn.metrics.accuracy_score"
    scorer_params {
      needs_proba = false
    }
  }
}
report {
  is_check_train = false
  error_handling = "fail"
  print_all_metrics = false
  metrics {
    accuracy {
      float_format = "{:.3f}"
      baseline_key = [
        "lgbm"
        "mles_embeddings"
      ]
      split_columns = true
    }
  }
}
auto_features = [
  "../data/emb__hidden_size_*.pickle"
]